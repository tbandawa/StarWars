<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/resources_navigation"
    app:startDestination="@+id/nav_resources">

    <fragment
        android:id="@+id/nav_resources"
        android:name="starwars.resources.ui.ResourcesFragment"
        tools:layout="@layout/resources_fragment">
        <argument
            android:name="resourceType"
            app:argType="string" />
        <deepLink app:uri="starwars://resources/{resourceType}" />
    </fragment>

</navigation>