<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/resource_navigation"
    app:startDestination="@+id/nav_resource">

    <fragment
        android:id="@+id/nav_resource"
        android:name="starwars.resource.ui.ResourceFragment"
        tools:layout="@layout/resource_fragment">
        <argument
            android:name="resourceId"
            app:argType="integer" />
        <argument
            android:name="resourceType"
            app:argType="string" />
        <deepLink app:uri="starwars://resource/{resourceType}/{resourceId}" />
    </fragment>

</navigation>